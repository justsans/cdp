<%- model_class = FeedbackSection -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:feedback_id) %></dt>
      <dd><pre class="prettyprint"><%= @feedback_section.feedback_id %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:question) %></dt>
      <dd><pre class="prettyprint"><%= @feedback_section.question %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:answer) %></dt>
      <dd><pre class="prettyprint"><%= @feedback_section.answer %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:rating) %></dt>
      <dd><pre class="prettyprint"><%= @feedback_section.rating %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:question_type) %></dt>
      <dd><pre class="prettyprint"><%= @feedback_section.question_type %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:action_plan) %></dt>
      <dd><pre class="prettyprint"><%= @feedback_section.action_plan %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:category) %></dt>
      <dd><pre class="prettyprint"><%= @feedback_section.category %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              feedback_sections_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_feedback_section_path(@feedback_section), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              feedback_section_path(@feedback_section),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>